    "N,M = map(int,input().split())\n",
    "\n",
    "li = []\n",
    "for m in range(M):\n",
    "    li += list(map(int,input().split()))\n",
    "\n",
    "for n in range(1,N+1):\n",
    "    print(li.count(n))"
