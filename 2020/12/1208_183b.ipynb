   "source": [
    "# 点(Sx, Sy), (Gx, Gy)の入力\n",
    "Sx, Sy, Gx, Gy = list(map(int, input().split()))\n",
    "\n",
    "# 2点を通る直線の公式 y-Sy = ((Gy- Sy) / (Gx - Sx))(x-Sx)\n",
    "# 求めたい点はy座標が0の時のxなので\n",
    "\n",
    "# 反射前のy座標を反転させる\n",
    "Sy = -Sy\n",
    "\n",
    "# 公式を当てはめる\n",
    "# -Sy / (Gy- Sy) = (x-Sx)\n",
    "x = ((-Sy / (Gy- Sy)) * (Gx - Sx)) + Sx\n",
    "\n",
    "print(x)"
   ]
