    "N = int(input())\n",
    "X = list(map(int, input().split()))\n",
    "\n",
    "m = 0\n",
    "u = 0\n",
    "c = 0\n",
    "\n",
    "for x in X:\n",
    "    m += abs(x)\n",
    "    u += (x**2)\n",
    "    c = max(abs(c), abs(x))\n",
    "    \n",
    "print(m)\n",
    "print(u**0.5)\n",
    "print(c)"
