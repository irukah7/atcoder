    "import math\n",
    "# 入力\n",
    "N, D = map(int,(input().split()))\n",
    "\n",
    "cnt = 0\n",
    "for i in range(N):\n",
    "    X, Y = map(int,(input().split()))\n",
    "    dis = math.sqrt(X*X + Y*Y)\n",
    "\n",
    "    if dis <= D:\n",
    "        cnt += 1\n",
    "print(cnt)"
