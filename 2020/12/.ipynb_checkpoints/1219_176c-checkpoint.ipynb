    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "cnt = 0\n",
    "for i in range(N-1):\n",
    "    if A[i] > A[i+1]:\n",
    "        cnt += A[i]-A[i+1]\n",
    "        A[i+1] = A[i]\n",
    "        \n",
    "print(cnt)"
