   "source": [
    "import math\n",
    "\n",
    "N = int(input())\n",
    "A = list(map(int, input().split()))\n",
    "\n",
    "ans = A[0]\n",
    "# 最小公倍数を求める\n",
    "for i in range(1, N):\n",
    "    ans = ans * A[i] // math.gcd(ans, A[i])\n",
    "\n",
    "# 最小公倍数から1引く\n",
    "ans = ans-1\n",
    "\n",
    "suma = 0\n",
    "for a in A:\n",
    "    suma += ans%a\n",
    "print(suma)"
   ]
